{
  "name": "myonlycloud",
  "version": "0.0.9",
  "description": "myonly.cloud javascript/typescript interface",
  "author": "Sergey Chernov <real.sergeych@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "link": "yarn build && cd dist && yarn link",
    "build": "tsc",
    "full_build": "tsc && yarn docs",
    "docs": "typedoc --excludePrivate",
    "test": "jest --coverage",
    "pub": "yarn build && yarn publish && bin/pubdocs"
  },
  "bin": {},
  "entry": {
    "myocloud": "./src/index.ts",
    "myocloud.min": "./src/index.ts"
  },
  "output": {
    "library": "myocloud"
  },
  "dependencies": {
    "isomorphic-fetch": "^3",
    "unicrypto": "^1.5.5",
    "uparsecjs": "^0.2.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.14.0",
    "@babel/types": "^7.14.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "form-data": "^4.0.0",
    "husky": "^6.0.0",
    "jest": "^25.5.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
